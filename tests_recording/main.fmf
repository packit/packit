require:
- python3-flexmock
- python3-requre
- python3-pytest
- packit
- python3-packit
component:
- packit
tier: 2
tag:
- requre
test: pytest-3 -v .
duration: 10m
/test_api.py:
    /ProposeUpdate:
        /test_changelog_sync:
            summary: Bump version two times and see if the changelog is synced
            description: "        Bump version two times and see if the changelog\
                \ is synced\n        when it's configured to do so.\n        "
            test: python3 -m pytest -m '' -v test_api.py::ProposeUpdate::test_changelog_sync
        /test_comment_in_spec:
            summary: change specfile little bit to have there some change, do not
                increase version
            description: "        change specfile little bit to have there some change,\
                \ do not increase version\n        "
            test: python3 -m pytest -m '' -v test_api.py::ProposeUpdate::test_comment_in_spec
        /test_version_change_exception:
            summary: check if it raises exception, because sources are not uploaded
                in distgit
            description: "        check if it raises exception, because sources are\
                \ not uploaded in distgit\n        Downgrade rebasehelper to version\
                \ < 0.24.0\n        "
            test: python3 -m pytest -m '' -v test_api.py::ProposeUpdate::test_version_change_exception
        /test_version_change_exception_new_rebase_helper:
            summary: check if it raises exception, because sources are not uploaded
                in distgit
            description: "        check if it raises exception, because sources are\
                \ not uploaded in distgit\n        New version raises same error,\
                \ but it raises it on another place\n        Upgrade rebase helper\
                \ >= 0.24.0\n        "
            test: python3 -m pytest -m '' -v test_api.py::ProposeUpdate::test_version_change_exception_new_rebase_helper
        /test_version_change_mocked:
            summary: version is not not uploaded, so skip in this test
            description: "        version is not not uploaded, so skip in this test\n\
                \        "
            test: python3 -m pytest -m '' -v test_api.py::ProposeUpdate::test_version_change_mocked
/test_base_git.py:
    /ProposeUpdate:
        /test_download_remote_sources_via_spec:
            summary: 'Use case: package_config.sources and Source0 are out of sync,'
            description: "        Use case: package_config.sources and Source0 are\
                \ out of sync,\n        make sure packit downloads correct archive\
                \ specifiec in the spec file\n        "
            test: python3 -m pytest -m '' -v test_base_git.py::ProposeUpdate::test_download_remote_sources_via_spec
/test_status.py:
    /TestStatus:
        /test_copr_builds:
            summary: test_status.py TestStatus test_copr_builds
            test: python3 -m pytest -m '' -v test_status.py::TestStatus::test_copr_builds
        /test_distgen_versions:
            summary: test_status.py TestStatus test_distgen_versions
            test: python3 -m pytest -m '' -v test_status.py::TestStatus::test_distgen_versions
        /test_dowstream_pr:
            summary: test_status.py TestStatus test_dowstream_pr
            test: python3 -m pytest -m '' -v test_status.py::TestStatus::test_dowstream_pr
        /test_koji_builds:
            summary: test_status.py TestStatus test_koji_builds
            test: python3 -m pytest -m '' -v test_status.py::TestStatus::test_koji_builds
        /test_status:
            summary: test_status.py TestStatus test_status
            test: python3 -m pytest -m '' -v test_status.py::TestStatus::test_status
        /test_up_releases:
            summary: test_status.py TestStatus test_up_releases
            test: python3 -m pytest -m '' -v test_status.py::TestStatus::test_up_releases
        /test_updates:
            summary: test_status.py TestStatus test_updates
            test: python3 -m pytest -m '' -v test_status.py::TestStatus::test_updates
